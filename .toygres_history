+    (random() * 1000)::bigint,
+    gen_random_uuid(),
+    (ARRAY['login','logout','purchase','view','click','search'])[floor(random()*6)+1],
+    (ARRAY['web','mobile','api'])[floor(random()*3)+1],
+    (ARRAY['desktop','mobile','tablet'])[floor(random()*3)+1],
+    (ARRAY['linux','windows','macos','android','ios'])[floor(random()*5)+1],
+    (ARRAY['US','IN','DE','BR','JP','GB'])[floor(random()*6)+1],
+    (ARRAY['CA','TX','KA','MH','SP','BY'])[floor(random()*6)+1],
+    inet '192.168.1.1' + (random()*1000)::int,
+    (random()*2000)::int,
+    random() > 0.1,
+    (random()*5000)::int,
+    round((random()*500)::numeric, 2),
+    jsonb_build_object(
+        'feature_flag', (random() > 0.5),
+        'experiment_group', (ARRAY['A','B','C'])[floor(random()*3)+1]
+    ),
+    now() - (random() * interval '30 days')
+FROM generate_series(1, 1000);

